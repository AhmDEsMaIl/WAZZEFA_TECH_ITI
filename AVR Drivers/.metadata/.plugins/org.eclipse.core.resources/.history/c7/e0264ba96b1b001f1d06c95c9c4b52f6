int main(void)
{
	/*************************************************************/
	/*				         2- PIN SETUB				         */
	/*************************************************************/
	DIO_u8SetPinDirection(DIO_PORTD,DIO_PIN0,DIO_PIN_OUTPUT);
	DIO_u8SetPinDirection(DIO_PORTD,DIO_PIN1,DIO_PIN_OUTPUT);


	/*************************************************************/
	/*				 1- MODULES INITIALIZATION				     */
	/*************************************************************/
	USART_voidInit();
	/*************************************************************/
	/*				    3- SET CALLBACK FUNCTION				 */
	/*************************************************************/
	/***************************************************************/
	u8 Recvice=0;
	while(1)
	{
		USART_u8SendData();
	}
	return 0;
}


